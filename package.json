{
  "name": "jupyternim-labextension",
  "version": "0.1.0",
  "description": "Jupyter lab extension for nim/jupyternim. Provides syntax highlighting.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "nim",
    "jupyternim",
    "jupyterlab-extension"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stisa/jupyternim-labextension.git"
  },
  "author": "stisa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stisa/jupyternim-labextension/issues"
  },
  "homepage": "https://github.com/stisa/jupyternim-labextension#readme",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": ">=1.2.6",
    "@types/codemirror": "0.0.97"
  },
  "peerDependencies": {
    "codemirror": "~5.39.0",
    "crypto": "^1.0.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "rimraf": "~3.0.2",
    "typescript": "~3.8.3"
  },
  "files": [
    "{lib,style}/**/*.{css,d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}"
  ],
  "jupyterlab": {
    "extension": true
  }
}
